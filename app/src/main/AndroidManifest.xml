<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seimun.mobile_health">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisteredActivity"
            android:label="@string/title_activity_registered"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PersonalCenterActivity"
            android:label="@string/title_activity_personal_center"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ServiceDetailsActivity"
            android:label="@string/title_activity_service_details"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Hypertension_follow_up"
            android:label="@string/title_activity_hypertension_follow_up"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Diabetes_follow_up"
            android:label="@string/title_activity_diabetes_follow_up"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Neonatal_family_visit"
            android:label="@string/title_activity_neonatal_family_visit"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
